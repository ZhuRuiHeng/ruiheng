/**index.wxss**/
.container {
  padding: 0;
}
/*轮播*/
.swiper{
  width: 750rpx;
  height: 416rpx;
}
.swiper swiper-item image{
  display: block;
  width: 100%;
  height: 100%;
}
.img_tip{
  position: absolute;
  width: 750rpx;
  height: 100rpx;
  background: rgba(0, 0, 0, 0.2);
  margin-top: -100rpx;
  color: #fff;
  font-size: 30rpx;
  box-sizing: border-box;
  padding:10rpx;
  text-align: center;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
/* 搜索 */
.search{
  width: 90%;
  margin: 20rpx auto;
  display: flex;
  justify-content: flex-start;
  border:1px solid #e5e5e5;
  padding: 5rpx 10rpx;
  border-radius: 5px;
  background: #fff;
}


.search input{
    font-size: 24rpx;
}
.search .icon-search image{
    width: 42rpx;
    height:42rpx;
    vertical-align: middle;
}
.weui-cells.weui-cells.after-title{ 
  width: 90%;
}
/*最新最热切换  */
.main{width:100%;background: #fff;position: relative;}
.main:after {
  content:" ";
  display: inline-block;
  width:200%;
  position:absolute;
  top:0;
  left:0;
  border:1px solid #e8e8e8;
  -webkit-transform:scale(0.5);
  transform:scale(0.5);
  -webkit-transform-origin:0 0;
  transform-origin:0 0;
  box-sizing:border-box;
}
.nav{width:100%;height:68rpx;display:flex;flex-direction:row;}
.default{line-height:68rpx;text-align:center;flex:1;color:#000;font-size:28rpx;}
.red{line-height:68rpx;text-align:center;color:#fc5558;flex:1;font-size:28rpx;}
.red span{display:inline-block;padding:0 20rpx;
 /* border-bottom:2px solid #ff4444;  */
}

.show{display:block;}
.hidden{display:none;}
.service-list{
	padding: 0 20rpx;
}
.service-item{
	display: flex;
    position: relative;
}
.service-item:after{
  content:" ";
  display: inline-block;
  width:200%;
  position:absolute;
  top:0;
  left:0;
  border:1px solid #e8e8e8;
  -webkit-transform:scale(0.5);
  transform:scale(0.5);
  -webkit-transform-origin:0 0;
  transform-origin:0 0;
  box-sizing:border-box;
}
.service-img{
	width: 180rpx;
	height: 180rpx;
}
.service-item image{
	width: 160rpx;
	height: 160rpx;
    margin:10rpx 0 0 0;
}
.service-text{
	flex: 1;
	margin-left: 20rpx;
	width: 100%;
}
.service-text .service-tite{
	color: #000000;
	width: 100%;
	font-size: 24rpx;
    padding-top:20rpx;
    height: 76rpx;
    display: -webkit-box;  
    display: -moz-box;  
    overflow: hidden;  
    text-overflow: ellipsis;  
    word-break: break-all;  
    -webkit-box-orient: vertical;  
    -webkit-line-clamp:2;  
}
.service-tags{
	margin: 6rpx 0;
}
.service-tags text{
	padding: 6rpx 16rpx;
	border: 1rpx solid #f7982a;
	line-height: 1;
	color: #f7982a;
	font-size: 28rpx;
	border-radius: 4rpx;
	margin-right: 10rpx;
	margin-bottom: 10rpx;
	white-space: nowrap;
}
.service-city{
	color: #666;
}
.caozuo{
  padding-top: 20rpx;
  display: flex;
  justify-content:space-between;
  height: 63rpx;
  line-height: 63rpx;
}
.caozuo .service-price{
    color:#ff4444;
    font-size:26rpx;
}
.caozuo .service-gouwu image{
    width: 56rpx;
    height: 56rpx;
}
.service-text .buy-response {
    position: absolute;
    right: 0rpx;
    bottom: 0rpx;
    height: 120rpx;
    width: 120rpx;
    opacity: 0;
}
/*模块  */
.medit{
    width:100%;
    background: #fff;
    margin-top: 20rpx;
    text-align: center;
}
.medit .center{
    text-align: center;
    display: flex;
    padding: 30rpx 0;
    justify-content:center;
}
.nextUrl .medit .title{
    font-size: 30rpx;
}
.medit .center .all{
    border:1px solid #e5e5e5;
    padding:2rpx 0rpx 2rpx 10rpx;
    font-size: 22rpx;
    color:#8a8a8a;
    height: 30rpx;
    margin: 4rpx 0 0 10rpx;
}
.medit .center .all image{
    width: 30rpx;
    height: 32rpx;
    vertical-align: middle;
}
.caozuo .service-gouwu{
    position:relative;
    bottom:20rpx;
}
/*购物车弹窗  */
.bg{
    width: 100%;
    height: inherit;
    background: #000;
    position: fixed;
    bottom: 0;
    left: 0;
    opacity: 0.6;
    z-index: 6;
    overflow: hidden;
}
.carBox{
    width: 100%;
    background: #fff;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 9;
}
.carBox .top{
    display: flex;
}
.carBox .padding{
    width: 90%;
    margin: 0 auto
}
.carBox .top .left{
    width: 140rpx;
    height: 140rpx;
}
.carBox .top .left image{
   border-radius: 10rpx;
    border:1px solid #fff;
    position: relative;
    top:-20rpx;
}
.carBox .top .left image,.carBox .top .del image{
    width: 100%;
    height: inherit;
}
.carBox .top .right{
    flex: 1;
    padding:20rpx 0 0 0;
}
.carBox .top .right text,.carBox .mid .buy text{
    display: block;
    font-weight: normal;
    font-size:24rpx;
    line-height:40rpx;
}
.carBox .top .del{
    width: 40rpx;
    height: 40rpx; 
    padding: 20rpx;  
}
.carBox .top .right{
    display:block;white-space:nowrap; overflow:hidden; text-overflow:ellipsis;width: 100%;
    padding-left:20rpx;
}
.carBox .top .right text.price{
    color:#ff4444;
    padding-top:10rpx;
}
.carBox .top .shuoming{
    white-space:nowrap; overflow:hidden; text-overflow:ellipsis;
}
.carBox .mid{
    display: flex;
    justify-content:space-between;
    padding: 20rpx 0;
    position: relative;
}
.carBox .mid:after{
    content:" ";
    display: inline-block;
    width:200%;
    position:absolute;
    top:0;
    left:0;
    border:1px solid #e5e5e5;
    -webkit-transform:scale(0.5);
    transform:scale(0.5);
    -webkit-transform-origin:0 0;
    transform-origin:0 0;
    box-sizing:border-box;
}
.carBox .mid .num{
    display: flex;
    position:relative;
    top:10rpx;
}
.carBox .mid .num button,.carBox .mid .num input{
    display: inline-block;
    width: 60rpx;
    height: 60rpx;
    line-height: 60rpx;
    padding:0;
    text-align: center;
}
.carBox .mid .num input{
    font-size: 24rpx;
    border:1rpx solid #e5e5e5;
    border-left: 0;
    border-right:0;
    box-sizing: border-box;
}
.carBox .mid button{
    border-radius:none;
    font-size: 24rpx;
    border:1rpx solid #e5e5e5; 
}
.carBox button::after{
    content:none;
}
 .carBox .mid button.reduce{
    border-radius: 5rpx 0 0 5rpx;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
}
.carBox .mid button.add{
    border-radius: 0 5rpx 5rpx 0;
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
} 
.normal{ 
   color: black; 
} 
.disabled{ 
   color: #ccc; 
} 
.carBox .mid .gray{
   color:#949292;
}
.btn{
    display: flex;
}
.btn button{
    flex: 1;
    width:100%;
    font-size:30rpx;
}
.btn .addcar{
    background: #ff8855;
    color:#fff;
    border-radius: 0;
}
.btn .buy{
    background: #ff4444;
    color:#fff;
    border-radius: 0;
}
.nextUrl{
    width: 100%;
}
.kuanshi .title{
    font-size: 28rpx;
    padding: 20rpx 0;
}
.kuanshi .leibie{
    margin:0 0 30rpx 0;
}
.kuanshi .leibie text{
    font-size: 24rpx;
    border:1rpx solid #e5e5e5; 
    border-radius: 6rpx;
    padding: 10rpx; 
    box-sizing: border-box;
    margin-right:10rpx;
    position: relative;
}
.kuanshi .leibie text.white{
    background: #fff;
    color:#000;
}
.kuanshi .leibie text.red{
    background: #ff4444;
    border-color:#ff4444;
    color:#fff;
}